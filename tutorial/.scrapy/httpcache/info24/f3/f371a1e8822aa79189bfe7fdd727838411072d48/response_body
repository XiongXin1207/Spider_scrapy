<script type="text/javascript">var cnzz_protocol = (("https:" == document.location.protocol) ? " https://" : " http://");document.write(unescape("%3Cspan id='cnzz_stat_icon_1260618339'%3E%3C/span%3E%3Cscript src='" + cnzz_protocol + "s4.cnzz.com/z_stat.php%3Fid%3D1260618339%26show%3Dpic1' type='text/javascript'%3E%3C/script%3E"));</script></body>
<style>
.item4{width:120px;height:120px;overflow:hidden;position:relative;cursor:pointer;}
.item4 .caption{width:120px;height:120px;color:#fff;font-weight:normal;position:absolute;text-align:center;padding-top:50px;display:none;background:url('../../r/cms/www/default/img/pic/demo02.png') no-repeat;}
.item4 p{font-size:14px;line-height:20px;color:#fff;}
.item4 img{border:0;position:absolute;width: 100%;height: 100%}

</style>
<script>
$(function(){
	var move = -50;
	var zoom = 1.5;
	$(".item4").each(function(){
		var that=this
		$(that).bind({
			mouseenter:function(){
				item4Timer=setTimeout(function(){
					width = $(that).width()*zoom;
					height = $(that).height()*zoom;
					$(that).find('img').animate({'width':width, 'height':height, 'top':move, 'left':move},500);
					$(that).find('div.caption').fadeIn(500);
					},200);
			},
			mouseleave:function(){
				clearTimeout(item4Timer);
				$(that).find('img').animate({'width':$(that).width(), 'height':$(that).height(), 'top':'0', 'left':'0'},500);	
				$(that).find('div.caption').fadeOut(500);
				}
		});
	})	
});
</script>
