<style>
.item4{width:170px;height:170px;overflow:hidden;position:relative;cursor:pointer;}
.item4 .caption{width:170px;height:170px;color:#fff;font-weight:normal;position:absolute;text-align:center;padding-top:70px;display:none;background:url('../../r/cms/www/default/img/pic/demo02.png') no-repeat;}
.item4 p{font-size:16px;line-height:20px;color:#fff;border: 2px solid #fff;width: 85px;margin-left: 45px}
.item4 img{border:0;position:absolute;width: 100%;height: 100%}
</style>
<script>
$(function(){
	var move = -50;
	var zoom = 1.5;
	$(".item4").each(function(){
		var that=this
		$(that).bind({
			mouseenter:function(){
				item4Timer=setTimeout(function(){
					width = $(that).width()*zoom;
					height = $(that).height()*zoom;
					$(that).find('img').animate({'width':width, 'height':height, 'top':move, 'left':move},500);
					$(that).find('div.caption').fadeIn(500);
					},200);
			},
			mouseleave:function(){
				clearTimeout(item4Timer);
				$(that).find('img').animate({'width':$(that).width(), 'height':$(that).height(), 'top':'0', 'left':'0'},500);	
				$(that).find('div.caption').fadeOut(500);
				}
		});
	})	
});
</script>
