$(document).click(function(event){
    if($(".header-search-box").attr("show")=="true"){
        $(".header-search-box").animate({'right':'-382','opacity':'0'},1000);
    }
    event.stopPropagation();
})


function keyup_submit(e){ 
   var evt = window.event || e; 
    if (evt.keyCode == 13){
     //回车事件
     search()
   }
 }
