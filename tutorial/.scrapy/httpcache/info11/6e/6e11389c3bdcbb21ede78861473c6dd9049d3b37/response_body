<script>
        var liulanqiwidth=$(window).width();
        var orign_height;
        var orign_width;
        function  chushihua() {
            // $(".viewer").zoomer();
            $(".viewer").css({"width":liulanqiwidth});

            var outerLineHeight = document.documentElement.clientHeight - 120;
            $(".viewer").height(outerLineHeight + "px");
            var image1 = new Image();
            image1.src = $("#img").attr("src");
            //alert(image1.src);
            var imgHeightTemp = image1.height;
            var imgWidthTemp = image1.width;
            //alert(imgHeightTemp);
            //alert(imgWidthTemp);

            orign_height = image1.height;
            orign_width = image1.width;
            var rate = imgHeightTemp / imgWidthTemp;
            if (imgHeightTemp > outerLineHeight) {
                imgHeightTemp = outerLineHeight;
                imgWidthTemp = outerLineHeight / rate;
            }
            var imgTagle = document.getElementById("img");
            imgTagle.height = imgHeightTemp;
            imgTagle.width = imgWidthTemp;
            //alert(imgHeightTemp);
            //alert(imgWidthTemp);

            if ($.browser.msie) {
                $("#img").bind("mousewheel", function(e) {
                    var e = e || event, v = e.wheelDelta || e.detail;
                    if (v > 0)
                        resizeImg(this, false); //放大图片呗 
                    else
                        resizeImg(this, true); //缩小图片喽 
                    window.event.returnValue = false; //去掉浏览器默认滚动事件 
                    //e.stopPropagation(); 
                    return false;
                })
            }
            else {
                $("#img").bind("DOMMouseScroll", function(event) {
                    if (event.detail < 0)
                        resizeImg(this, false);
                    else
                        resizeImg(this, true);
                    event.preventDefault()//去掉浏览器默认滚动事件 

                    //event.stopPropagation(); }) 
                });
            }

            document.onmousedown = selectmouse;
            document.onmouseup = new Function("isdrag=false");


        };

        chushihua();




        function resizeImg(node, isSmall) {
            if (!isSmall) {
                if ($(node).height() / orign_height <= 1) {
                    $(node).height($(node).height() * 1.1);
                    $(node).width($(node).width() * 1.1);
                }
            }
            else {
                if ($(node).height() / orign_height > 0.3) {
                    $(node).height($(node).height() * 0.9);
                    $(node).width($(node).width() * 0.9);
                }
            }
        }


        function da() {
            resizeImg(document.getElementById("img"), false);
        }

        function xiao() {

            resizeImg(document.getElementById("img"), true);
        }

        function re() {
            location.href = location.href;
        }
        var ie = document.all;
        var nn6 = document.getElementById && !document.all;
        var isdrag = false;
        var x, y;
        var dobj;
        function movemouse(e) {
            if (isdrag) {
                dobj.style.left = (nn6 ? tx + e.clientX - x : tx + event.clientX - x) + 'px';
                dobj.style.top = (nn6 ? ty + e.clientY - y : ty + event.clientY - y) + 'px';
                return false;
            }
        }

        function selectmouse(e) {
            var fobj = nn6 ? e.target : event.srcElement;
            var topelement = nn6 ? "HTML" : "BODY";
            while (fobj.tagName != topelement && fobj.className != "dragme") {
                fobj = nn6 ? fobj.parentNode : fobj.parentElement;
            }
            if (fobj.className == "dragme") {
                isdrag = true;
                dobj = fobj;
                tx = parseInt(dobj.style.left + 0);
                ty = parseInt(dobj.style.top + 0);
                x = nn6 ? e.clientX : event.clientX;
                y = nn6 ? e.clientY : event.clientY;
                document.onmousemove = movemouse;
                return false;
            }

        }
     
    </script>



