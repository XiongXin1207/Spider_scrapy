                var windowwidth=$(window).width();
                if(windowwidth<1200){
                    $(".header,.footer").css("width","1200px");
                }else{
                    $(".header,.footer").css("width","100%");
                }
                $(window).resize(function(){
                    var windowwidth=$(window).width();
                    if(windowwidth<1200){
                        $(".header,.footer").css("width","1200px");
                    }else{
                        $(".header,.footer").css("width","100%");
                    }

                });
                /*导航弹出层 开始*/

                $('.nav_eject_Bul .nav_eject_Bli:last-child').css('border','0');
