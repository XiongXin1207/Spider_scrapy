                    //mode:'vertical', //默认的是水平
                    displaySlideQty: 1, //显示li的个数
                    moveSlideQty: 1, //移动li的个数
                    captions: true, //自动控制
                    auto: true,
                    controls: true, //隐藏左右按钮
                    autoHover: true
                });
                jQuery(document).ready(function ($) {
                    $(".fancybox").fancybox({
                        openEffect: 'none',
                        closeEffect: 'none',

                        helpers: {
                            title: {
                                type: 'inside'
                            }
                        }
                    });
                });
            </script>
            </body></html>