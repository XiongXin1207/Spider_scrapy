         // $('.banner ul li').height(img_h);
        //  $('.banner ul').height(img_h);
         // $('.banner ol li').height(img_h2);
         // $('.banner ol').height(img_h2);
         // $('.banner').height(img_h+img_h2);
         // $('.banner a').css('top',img_h/2)
        
        //setInterval(go,200);

        //var mya = $('.banner ul li:eq(0)').clone()
       //$('.banner ul').append(mya);

        //默认值
	var num2 = 0;
	var num = 0;
	var timer;
        var nummax=$('.banner ul li').length-1;
        var nummax2=$('.banner ol li').length-1;
        var ddw=$('.banner ul li').width();
        var liw=$('.banner ol li').width()+20;
        var imgh=$('.banner ul li').height();
        var len_b=$('.banner ul li').length;
        if(nummax<=1){$('.banner a.prev,.banner a.next').hide();}
        $('.banner ol li').eq(0).addClass('current')
	//角标的工作
	$('.banner ol li').mouseenter(function(e) {
             $(this).addClass('current').siblings().removeClass('current')
	     var selfIndex = $(this).index()
	     $('.banner ul').stop().animate({'left':-selfIndex*ddw},400)
             num= selfIndex;
              $('.banner ul li img').css('zoom','100%');
        });
	//自动轮播
        
	function autoPlay(){
		num++;
                num2++
		if(num>nummax){
			num=1
			$('.banner ul').stop().css({'left':0})
		}
		if(num2>nummax2){
			num2=0
		}
                if(nummax<=1){clearInterval(timer)}
                $('.banner ol li').eq(num2).addClass('current').siblings().removeClass('current')
		$('.banner ul').stop().animate({'left':-num*ddw},400)
                if(nummax2>3){
                  if(num2>3){
                    $('.banner ol').stop().animate({'left':-(num2-3)*liw},400);
                  }else{
                    $('.banner ol').stop().animate({'left':0},400);
                  }

                }



	}
	//timer = setInterval(autoPlay,8000)
        
        //if(nummax<=1){clearInterval(timer)}	
		
	//鼠标移上关闭定时器
	$('.banner').hover(function(){
	     //clearInterval(timer)
		
	},function(){
	    // timer = setInterval(autoPlay,8000)
             var len=$('.banner ul li').length;
           //  if(nummax<=1){clearInterval(timer)}
		
	})
	
	
	/*按钮*/
	$('.banner a.next').click(function(e) {
              autoPlay();
              $('.banner ul li img').css('zoom','100%');
        });
	$('.banner a.prev').click(function(e) {
                num--;
                num2--;
		if(num<0){
		    num=nummax-1
		}
		if(num2<0){
		    num2=nummax2
		}
                $('.banner ol li').eq(num).addClass('current').siblings().removeClass('current');
		$('.banner ul').stop().animate({'left':-num*ddw},400);
                $('.banner ul li img').css('zoom','100%');

                if(nummax2>3){
                  if(num2>3&&num2!=nummax){
                    $('.banner ol').stop().animate({'left':-(num2-3)*liw},400);
                  }else{
                    $('.banner ol').stop().animate({'left':0},400);
                  }

                }
	
         });

})	

      

</script> 
