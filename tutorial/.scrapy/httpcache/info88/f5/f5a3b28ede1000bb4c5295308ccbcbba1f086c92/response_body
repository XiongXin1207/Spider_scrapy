function bigimg(i)
{
	var zoom = parseInt(i.style.zoom,10)||100;
        zoom += event.wheelDelta / 12;
        var img_h=$('.banner ul li').eq(0).height();
        var hh=img_h*zoom/100;
        var tc_h=tchh*zoom/100;
        var tc_w=tcww*zoom/100;
	if(zoom > 0&&img_h>0 ){

	   i.style.zoom=zoom+'%';
   
        
          $('.banner ul li').find('.li_cont').css({'margin-top':-hh/2,'height':tc_h,'top':'50%'});
            return false;
        }

        
}
function bigimg1(i)
{
	var zoom = parseInt(i.style.zoom,10)||100;
        zoom += event.wheelDelta / 12;
        var img_h=$('.banner ul li').eq(0).height();
        var hh=img_h*zoom/100;
        var tc_h=tchh*zoom/100;
        var tc_w=tcww*zoom/100;
	if(zoom > 0&&img_h>0 ){

	  i.style.zoom=zoom+'%';
   

         $('.tushang ul li').find('.li_cont').css({'margin-top':-tc_h/2,'height':tc_h,'top':'50%'});
          //  return false;
        }

        
}
$(function(){
    $.each(imgHuilanMediaArray,function(){
        var src=this.src;
        var href;
        if(src.indexOf('attachment/')==0){
            href='/'+src;
        }else{
            href=src; 
        }
        var len= $('.banner ul li').length;
        var len1= $('.tushang ul li').length;
        if(len<1){
          $('ul.image').append('<li><div class="li_cont"  ondragstart="return false"><img src="'+href+'"  /></div></li>');
        }
           //$('ol.image').append('<li><img src="'+href+'"  /></li>');
        if(len1<1){ 
          $('.tushang  ul').append('<li > <div class="li_cont"  ondragstart="return false"> <img src="'+href+'"  onmousewheel="return bigimg1(this)"  /></div></li>');
        }
    });
    function CC(){
      var len_ul=$('ul.image li').length;
      if(len_ul==0){
         $.each(imgHuilanMediaArray,function(){
            var src=this.src;
            var href;
            if(src.indexOf('attachment/')==0){
               href='/'+src;
            }else{
               href=src; 
            }
            var len= $('.banner ul li').length;
            if(len<1){
               $('ul.image').append('<li><div class="li_cont"  ondragstart="return false"><img src="'+href+'"  onmousewheel="return bigimg(this)" /></div></li>');
               //$('ol.image').append('<li><img src="'+href+'"  /></li>');
               $('.tushang .ts_cont ul').append('<li > <div class="li_cont"  ondragstart="return false"> <img src="'+href+'"  onmousewheel="return bigimg(this)"  /></div></li>');
            }
          });
       }
    }



        //拖动
        var $this;
        var move=false;//移动标记 
        var _x,_y,x1,y1;//鼠标离控件左上角的相对位置 


        $('.tushang ul li .li_cont').mousedown(function(e){ 

           $this=$(this);
           move=true; 
          
           _x=e.pageX-parseInt($this.css("left")); 
           _y=e.pageY-parseInt($this.css("top")); 
        });

        $('.banner ul li img').mousedown(function(e){ 

           $this=$(this);
           move=true; 
          
           _x=e.pageX-parseInt($this.css("left")); 
           _y=e.pageY-parseInt($this.css("top")); 
        });
        $(document).mousemove(function(e){ 
          if(move){ 
            var x=e.pageX-_x;//控件左上角到屏幕左上角的相对位置 
            var y=e.pageY-_y; 

           $this.css({"top":y,"left":x}); 

           
          } 
        }).mouseup(function(){ 
          move=false; 
        });


    
});
</script> 
 <script type="text/javascript">
    $(function(){
    //点击精品图片
   // var mya = $('.ts_img ul li:eq(0)').clone();
    //$('.ts_img ul').append(mya);
    $('.banner ul li').click(function(){
         var i=$(this).index();
         $('.tushang').show(); 
         $('.tushang ul').stop().animate({'left':-tsw*i},0);

    })
    $('.texiao .max_big').click(function(){
         $('.tushang').show(); 
    });
   //点击隐藏精品图片
     $('.ts_remove').click(function(){
          $('.tushang').hide();
          tc()
     });
     $('.ts_remove').hover(function(){
          $(this).addClass('active');
     },function(){
          $(this).removeClass('active');

     });
     $('.ts_cont').mouseenter(function(){
         $('.ts_remove').addClass('cur');
     },function(){
         $('.ts_remove').removeClass('cur');

     });
    
     //显现大图
    var winw=$(window).width();
    var winh=$(window).height();
    var tsw=winw*0.7;
    var ts_h=winh*0.8;

    var ol_h=tsw/1.75;


   
    //$('.tushang ul li').css({'width':winw});


   //下载
   var load_url= $('.banner ul li').eq(0).find('img').attr('src');
   $('.texiao li.load a').attr('href',load_url)
   //属性
   var cp_list_i=0;
   $('.cp_list dl dd').each(function(){
         var txt=$(this).find('span').text();
         txt=txt.replace(/\s+/,'');
         if(txt==''){
            cp_list_i++;
            $(this).hide();
         }
         if(cp_list_i==6){
             $('.cp_list').hide();
         }
   })

   //来源
   var link=$('.js_title .info .link').text()
   try{var link1=link.replace(/\s+/,'') }catch(e){var link1=''}
   if(link1==''){
       $('.js_title .info .link').text('广东省博物馆')
   }
  //打印
  $('.texiao .print').click(function(){
       $('.top').css('position','absolute')
       window.print();
  })
})	
