  <script type="text/javascript">
    $(function(){





        //默认值
	var num2 = 0;
	var num = 0;
	var timer;
        var nummax=$('.ts_img ul li').length-1;
        var nummax2=$('.ts_img ol li').length-1;
        var ddw=$('.ts_img ul li').width();
        var imgh=$('.ts_img ul li').height();
        $('.ts_img ul').stop().css({'left':0})
        if(nummax<=1){$('.ts_img a.prev,.ts_img a.next').hide();}
	//角标的工作
	$('.ts_img ol li').mouseenter(function(e) {
             $(this).addClass('current').siblings().removeClass('current')
	     var selfIndex = $(this).index()
	     $('.ts_img ul').stop().animate({'left':-selfIndex*ddw},400)
             num= selfIndex;
             $('.ts_img ul li img').css('zoom','100%');
        });
	//自动轮播
	function autoPlay(){
		num++;
                num2++
		if(num>nummax){
			num=1
			$('.ts_img ul').stop().css({'left':0})
		}
		if(num2>nummax2){
			num2=0
		}
                if(nummax<=1){clearInterval(timer)}
                $('.ts_img ol li').eq(num2).addClass('current').siblings().removeClass('current')
		$('.ts_img ul').stop().animate({'left':-num*ddw},400)
                $('.ts_img ul li img').css('zoom','100%')
                

	}
	//timer = setInterval(autoPlay,10000)
	//if(nummax<=1){clearInterval(timer)}	
		
	//鼠标移上关闭定时器
	$('.ts_img').hover(function(){
		//clearInterval(timer)
		
	},function(){
		//timer = setInterval(autoPlay,10000)
		//if(nummax<=1){clearInterval(timer)}
	})
	
	
	/*按钮*/
	$('.ts_img a.next').click(function(e) {
              autoPlay();
             $('.ts_img ul li img').css('zoom','100%')
        });
	$('.ts_img a.prev').click(function(e) {
                num--;
                num2--;
		if(num<0){
		    num=nummax
		}
                
                $('.ts_img ol li').eq(num).addClass('current').siblings().removeClass('current')
		$('.ts_img ul').stop().animate({'left':-num*ddw},400)
                $('.ts_img ul li img').css('zoom','100%')
	
         });

})	


</script> 
  <script>
//iframe高度
 function pinglun_h(a){
     var b=a+10
     $('#pl_w .pl_cont iframe').height(b);
 }
 $(function(){
     var numm=1;
     $('#pl_w .pl_cont').show();
     $('#pl_w .pl_head').click(function(){
          if(numm==0){
              $('#pl_w .pl_cont').show();
              $('#pl_w').css('padding-bottom',5);
              numm=1;
          }else{
              $('#pl_w .pl_cont').hide();
              $('#pl_w').css('padding-bottom',0);
              numm=0;
          }
     })

 })
