 <script type="text/javascript">
//收藏
function collect(){
	//alert(1);
	var strurl ="/kisstask-weixin/web/collectArticle";
	var articleUrl=window.location.href;
	$.ajax({   
	     url:strurl,   
	     type:'post',
	     data:{articleUrl:articleUrl},
	     error:function(){   
	     },   
	     success:function(data){   
		var resultJson = eval("("+data+")");
                if(resultJson.status=="success"){
                    
                    if(resultJson.des=="collect"){
                        alert("收藏成功");
	    		$('.info .collect a').text('已收藏');
	    		$('.info .collect').addClass('cur');
                    }else{
                       alert("你已经收藏过这篇文章了！");
                         
                    }
                }else if(resultJson.status=="fail"){
                    alert("请登录");
                    $('.dl_ifram').show();
                }
	     }
	 });
}
var strurl1 ="/kisstask-weixin/web/isCollect";
	$.ajax({   
	     url:strurl1,   
	     type:'post',
	     data:{articleUrl:window.location.href},
	     error:function(){   
	     },
	     success:function(data){   
	    	var status = eval("("+$.trim(data)+")");
	    	if(status.status=="nologin"){
	    		//console.log("没有登录");
	    	}else if(status.status=="nocollect") {
	    		//console.log("没有收藏");
	    	}else if(status.status=="collect") {
	    		$('.info .collect a').text('已收藏');
	    		$('.info .collect').addClass('cur');
	    	}
	     }
       })
