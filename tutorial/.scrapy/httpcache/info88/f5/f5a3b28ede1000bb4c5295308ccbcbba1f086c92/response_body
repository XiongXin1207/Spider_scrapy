 $(function(){
    $('#list .list_ul img').each(function(){
        var str=$(this).attr('src');
        if(str.indexOf('/fileDir')==0){
           str=str.substr(8)
           $(this).attr('src',str);
        }
    })
    $('#detail1 .banner li img').each(function(){
      var src= $(this).attr('src');

      if(src.indexOf('attachment')==0){

         src='/'+src;
         $(this).attr('src',src)
        
      }
    })
    $('#content .product img').each(function(){
        var str=$(this).attr('src');
        if(str.indexOf('/fileDir')==0){
           str=str.substr(8)
           $(this).attr('src',str);
        }
    })
    $('img').each(function(){
        var str=$(this).attr('src');
        str=str.replace(/\s+/,'')

        if(str=='/'){
          $(this).hide();
          $(this).parent('.txt_img').hide();
          $(this).closest('li').find('.txt_cont').css('width',880);
          $(this).remove();
        }else if(str==''){
          $(this).hide()
          $(this).parent('.txt_img').hide();
          $(this).closest('li').find('.txt_cont').css('width',880);
          $(this).remove();
        }
    })
   


  //img兼容
  $('img').each(function(){
      var src= $(this).attr('src');
      try{
        if(src.indexOf('/fileDir')==0){
           src='/eportal'+src;
           $(this).attr('src',src);
         }else if(src.indexOf('fileDir')==0){
             src='/eportal/'+src;
             $(this).attr('src',src);
         }
       }catch(err){}



 
     var a=src;
     a.replace(/\s+/,'')
     if(src=='/'){
         $(this).hide();
     }
  });
  setInterval(function(){
       $('img').each(function(){

             var href=$(this).attr('src');

             if(href.indexOf('attachment')==0){
                 href='/'+href
                 $(this).attr('src',href);
             }
       })
  },300)

})


