<script>
$(function(){
  $('.top_nav .nav_ul .nli').first().find('.sub').hide();
  $('.top_nav .nav_ul .nli').eq(1).find('.sub').addClass('size')
  $('.top_nav .nav_ul .nli').eq(5).find('.sub').addClass('size2')
  $('.top_nav .nav_ul .nli').hover(function(){
    $(this).addClass('on').siblings().removeClass('on')
    var ind =$(this).index();
    if(ind<3){
      $(this).find('.sub').css({'left':0,'right':'auto','margin':0})
    }else if(ind>5){
      $(this).find('.sub').css({'left':'auto','right':0,'margin':0})
     }
  },function(){
    $(this).removeClass('on')
  })
  
  //设计背景

   var a= $('.navgationDiv').css('background');
   $('.navgationDiv').css('top',0)
    $('.logo').mouseenter(function(){
        $('.navgationDiv').stop().animate({'top':0},400)
   })
  $('.navgationDiv').mouseleave(function(){
        $('.navgationDiv').stop().animate({'top':0},400)
   })
  
 //详细页的登陆和注册
 $('#loginSpan a').attr('href','/gdmuseum/_300706/_300710/dl/index.html');
 $('#registHide a').attr('href','/gdmuseum/_300706/_300710/zc13/index.html')

