<script>
$(function(){
   $('.foot_nav .foot_wrap dl dd').each(function(){
      var ind = $(this).index();
      var p=$('.child_en span').eq(ind).text();
      var pw=$('.child_en span').eq(ind).width();
      $(this).append('<p>'+p+'</p>')
      $(this).find('p').css({'width':pw,'margin-left':-pw/2})
      
   })
   $('.child_en').css('display','none')

   //页码
   var font=$('.NormalRed').text();
   $('.NormalRed').after('<a class="cur_page" style="color:red;border:1px solid #A55656;">'+font+'</a>')
   $('.NormalRed').hide();



   //子站顶部链接
   $('#header_logo a').attr('href','/gdmuseum/index.html')

    //来源
   var link=$('.info .link').text()
   
   try{var link1=link.replace(/\s+/,'') }catch(e){var link1=''}
   try{var link2=link.index('广东') }catch(e){var link2=-1}
   try{var link3=link.index('博物') }catch(e){var link3=-1}
   if(link1==''||link2>0&&link3>0){
       $('.info .link').text('广东省博物馆');
   }
   //底部二维码
  $('.foot_ewm').load('/gdmuseum/zbgddh/dbwxewm/index.html #db_ewm')
  


})



