  <script>
$(function(){
   //搜索
   var sousuo=0;
   $('.cp_search dl dd').eq(0).show();
   $('.cp_search .search_head').click(function(){
       if(sousuo==0){
          $('.cp_search ol').show();
          $('.cp_search .search_head').addClass('cur');
          sousuo=1;
       }else{
          $('.cp_search ol').hide();
          $('.cp_search .search_head').removeClass('cur'); 
          sousuo=0;
       }
   })
 
   var cur_pid=362398;
   var cur_mid=$('#content_yugao .portlet').attr('id');
   var saction='/eportal/ui?pageId='+cur_pid+'&moduleId='+cur_mid+'&currentPage=1';
   var saction1="'"+saction+"'";
   var search_name=['filter_LIKE_TITLE','filter_LIKE_EXT_STR2','filter_LIKE_EXT_STR3','filter_LIKE_EXT_STR4','filter_LIKE_EXT_STR5','filter_LIKE_EXT_STR9']
   var idarr=['seartxt1','seartxt2','seartxt3','seartxt4','seartxt5','seartxt6']

   $('.cp_search dl dd').each(function(){
       var i=$(this).index();
       var txt=$('.cp_search ol li').eq(i).text();
       var str='<form action="'+saction+'" method="post" opentype="page">';
       str+='<input placeholder="'+txt+'" name="'+search_name[i]+'" type="text" class="searchtxt" id="'+idarr[i]+'" onblur="blurs(this)">'; 
       str+='<input class="searchbtn" onclick="queryArticleByCondition(this,'+saction1+')" value="搜索" type="button"></form>';
       $(this).html(str);
        
   })
   $('.cp_search ol li').click(function(){
       var i=$(this).index();
       var txt=$(this).text();
       $('.cp_search dl dd').eq(i).show().siblings().hide(); 
        
       $('.cp_search .search_head').text(txt);
       $('.cp_search ol').hide();
       $('.cp_search .search_head').removeClass('cur');
       sousuo=0;
   })

  $('.cp_search dl dd').each(function(){
       var url=window.location.href;
       try{var a=url.indexOf('&filter_LIKE_')}catch(e){var a=-1};
       var name=$(this).attr('name');
       var sear=$(this).attr('sear');

       if(a>=0){
           var x=url.split('&filter_LIKE_')[1];
           var b=x.split('=')[0];
           var c=x.split('=')[1];
               c=decodeURI(c);
           if(b==sear){

              $(this).show().siblings().hide();
              $('.cp_search .search_head').text(name);
              $(this).find('.searchtxt').val(c);
           }
       }
       
   })
  


})
   function blurs(obj){

        var cur_pid=362398;
        var cur_mid=$('#content_yugao .portlet').attr('id');
        var saction='/eportal/ui?pageId='+cur_pid+'&moduleId='+cur_mid+'&currentPage=1';
        var saction1="'"+saction+"'";
        var aa=obj.getAttribute("id");
        var bb=$('#'+aa)
     
        var zhi=bb.val();
        var name=bb.attr('name');
        var act=saction+'&'+name+'='+zhi;
        var act1="'"+act+"'";
  
        var ori=$('.cp_search dl').css('background-origin');
        if(ori=='content-box'){
          bb.parent('form').attr('action',act);
          bb.siblings('.searchbtn').attr("onclick","queryArticleByCondition(this,"+act1+")");
        }
       

    }
</script> 
  <script>
$(function(){
   var img_num=0;
   var ww_len=$('.js_cont .js_img').length;
   $('.js_cont .js_img').each(function(){
       var i=$(this).index()+1;
       var j=i%4;
       var tx1= $(this).find('.number .num1').attr('name');
       var txt2= $(this).find('.number .num2').attr('name');
       try{txt1=txt1.replace(/\s+/,'')}catch(e){txt1=''}
       try{txt2=txt2.replace(/\s+/,'')}catch(e){txt2=''}
       if(txt1!=''){
          img_num++
       }
       if(txt2!=''){
          img_num++
       }
 console.log(txt2+'a')
       if(j==0&&img_num!=0||ww_len<=4){
          $('.js_cont .js_img').eq(i-1).find('.number').show();
          $('.js_cont .js_img').eq(i-2).find('.number').show();
          $('.js_cont .js_img').eq(i-3).find('.number').show();
          $('.js_cont .js_img').eq(i-4).find('.number').show();
          img_num=0;
          
       }

     
   })

    $('.js_cont .js_img .number').each(function(){
       var num_b=0;
       $(this).find('b').each(function(){
          var txt=$(this).text();
          try{txt=txt.replace(/\s+/,'');}catch(e){}
          if(txt!=''){
              num_b++;
              if(num_b==2){
                  //$(this).addClass('cur');
              }
          }
       })
       
    })
   


    //翻页显隐
    var js_img_len=$('.paging #page_num').text();
    if(js_img_len>40){
        $('.paging').show();
    }else{
        $('.paging').hide();

    }
})
