<script>
function IsPC() {
    var userAgentInfo = navigator.userAgent;
    var Agents = ["Android", "iPhone",
                "SymbianOS", "Windows Phone",
                "iPad", "iPod"];
    var flag = true;
    for (var v = 0; v < Agents.length; v++) {
        if (userAgentInfo.indexOf(Agents[v]) > 0) {
            flag = false;
            break;
        }
    }
    return flag;
}
 function isdypc(){
var flag = IsPC(); //true为PC端，false为手机端
  var now_url=window.location.href
  if(now_url.indexOf('weixin') > -1){
  	flag = true;
  }
  if(!flag){
   var now_url=window.location.href
   if(now_url.indexOf('?tz=1') > 0){
            $('a').each(function(){
               var a_h=$(this).attr('href')
              try{ var a_h2=a_h.replace(/\s+/);}catch(err){};
               if(a_h2!=''){
                try{
                  if(a_h.indexOf('eportal/ui?')>0){
                     a_h=a_h+'&tz=1';
                     $(this).attr('href',a_h);
                  }else if(a_h.indexOf('?tz=1')==-1){
                     a_h=a_h+'?tz=1';
                     $(this).attr('href',a_h);
                  }
                }catch(err){}
             }

          })

     }else{

          location.href="http://www.gdmuseum.com/webphone/";
     }
  	 
  }
  if(now_url.indexOf('?tz=66') > 0){$('.info_calendar').show();}
}
</script>
